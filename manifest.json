{
  "manifest_version": 3,
  "name": "Demo Extension",
  "description": "Getting URL Of Current Page.",
  "version": "1.0",
  "permissions": ["activeTab"],
  "icons": {
    "16": "quality.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.7.0.min.js", "popup.js"]
    }
  ],
  "action": {
    "default_popup": "demo.html",
    "default_icon": "quality.png"
  }
}
